---
const id = Math.random().toString(36).slice(2, 11);
---

<script id={"wrap-code-" + id} type="module" define:vars={{ id }} is:inline>
	const self = document.getElementById("wrap-code-" + id);
	const code = self.previousElementSibling;

	if (code) {
		code.classList.add("wrap-code");
	}
</script>
